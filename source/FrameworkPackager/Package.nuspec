    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>http://opensource.org/licenses/Apache-2.0</licenseUrl>
    <copyright>Copyright Igniteous Ltd 2015</copyright>
    <dependencies>
        <group targetFramework="netmf43">
          <dependency id="NetMf.CommonExtensions" version="1.1.2" />
          <dependency id="MicroLinq" version="" />
          <dependency id="AMQPNetLite" version="" />
          <dependency id="Json.NetMF" version="1.3.0.0" />
        </group>
    </dependencies>
    <references></references>
    <tags></tags>
  </metadata>
